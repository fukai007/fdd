<!--pages/collect/collect.wxml-->
<view class='mx60'>

    <!--  大分类区域  开始  -->
    <view class='df fxyc mt50'>
        <view class='bigTypeAreaCore df fsb'>
              <view class='df fdy fyc'><image class='wh50 bgc-red'></image><text class='mt30'>错题本</text></view>
              <view class='df fdy fyc'><image class='wh50 bgc-red'></image><text class='mt30'>我的收藏</text></view>
        </view>
    </view>
    <view class='mt50 hline'></view>
    <!--  大分类区域  结束  -->

    <!--  小分类区域  开始  -->
    <view class='df fww mt50'> 
      <view wx:for="{{tagList}}" wx:for-item="tagItem" wx:key="{{tagItem.id}}" class='ptype ml60 df fxyc' data-index='{{index}}'>
            <text>{{tagItem.name}}</text>
      </view>
    </view>
    <!--  小分类区域  结束  -->
    
     <!-- 错题或收藏题 区域 开始    -->
</view>
